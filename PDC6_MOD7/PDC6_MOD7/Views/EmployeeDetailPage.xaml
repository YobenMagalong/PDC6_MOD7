<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PDC6_MOD7.Views.EmployeeDetailPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="StudentId" Text="{Binding studentid}"/>
            <Entry x:Name="Name" Text="{Binding name}"/>
            <Entry x:Name="Course" Text="{Binding course}"/>
            <Entry x:Name="Year" Text="{Binding year}"/>
            <Entry x:Name="Section" Text="{Binding section}"/>
            <Button Text="Update" Clicked="BtnUpdate"/>
            <Button Text="Delete" Clicked="BtnDelete"/>


        </StackLayout>
    </ContentPage.Content>
</ContentPage>